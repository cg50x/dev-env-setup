
printHeader() {
	echo
	echo -------------------------------------------------------------------------------------------------------------------------------------------
	echo $1
	echo
}

printSubHeader() {
	echo
	echo -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
	echo $1
	echo
}

scheduleForNextRun() {
	echo $1 > ~/.onstart
}

SUDO=$(which sudo 2> /dev/null)

